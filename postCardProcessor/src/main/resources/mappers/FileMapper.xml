<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.soho.inko.mapper.FileMapper">

    <select id="getFilePathByFileId" resultType="java.lang.String">


        SELECT PATH
        FROM res_image_file
        WHERE ID                                  = #{fileId}

    </select>

</mapper>